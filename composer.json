{
    "name": "norman-huth/hellofresh-database",
    "type": "project",
    "description": "HelloFresh for different countries including features like filtering by ingredients, allergens, tags, etc.",
    "keywords": [
        "cooking",
        "framework",
        "hellofresh",
        "laravel",
        "recipes"
    ],
    "homepage": "https://hfresh.info",
    "support": {
        "source": "https://github.com/Muetze42/hellofresh-database",
        "issues": "https://github.com/Muetze42/hellofresh-database/issues",
        "docs": "https://github.com/Muetze42/hellofresh-database/discussions"
    },
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "^0.6.11",
        "laravel/framework": "^10.46",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "norman-huth/hellofresh-scraper": "@dev",
        "sentry/sentry-laravel": "^4.2",
        "spatie/laravel-translatable": "^6.6.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.12",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "norman-huth/helpers-collection-laravel": "^1.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "phpunitgen/console": "^6.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "functions/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpmd": [
            "phpmd app,database,config,routes text phpmd.xml"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "code-quality": [
            "./vendor/bin/pint",
            "phpmd app,database,config,routes text phpmd.xml"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
